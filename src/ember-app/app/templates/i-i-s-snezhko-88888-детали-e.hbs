<h3 class="ui header">{{t "forms.i-i-s-snezhko-88888-детали-e.caption"}}</h3>

<form class="ui form flexberry-vertical-form" role="form">
  {{flexberry-error error=error}}
  <div class="field">
    <div class="flexberry-edit-panel">
      {{#unless readonly}}
        <button class="ui button save-button" {{action "save"}}>
          {{t "forms.edit-form.save-button-text"}}
        </button>
        {{#unless model.isNew}}
          <button class="ui button save-del-button" {{action "delete"}}>
            {{t "forms.edit-form.delete-button-text"}}
          </button>
        {{/unless}}
      {{/unless}}
      <button class="ui button close-button" {{action "close"}}>
        {{t "forms.edit-form.close-button-text"}}
      </button>
    </div>
  </div>
  <div class="field flexberry-validationsummary-container">
    <div class="sixteen wide">
      {{flexberry-validationsummary errors=(v-get validationObject "messages")}}
    </div>
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.кодДетали
      class=(if (v-get validationModel "кодДетали" "isInvalid") "error")
      label=(t "forms.i-i-s-snezhko-88888-детали-e.кодДетали-caption")
      data-test-i-i-s-snezhko-88888-детали-e-кодДетали=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "кодДетали" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.наименование
      class=(if (v-get validationModel "наименование" "isInvalid") "error")
      label=(t "forms.i-i-s-snezhko-88888-детали-e.наименование-caption")
      data-test-i-i-s-snezhko-88888-детали-e-наименование=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "наименование" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.производитель
      class=(if (v-get validationModel "производитель" "isInvalid") "error")
      label=(t "forms.i-i-s-snezhko-88888-детали-e.производитель-caption")
      data-test-i-i-s-snezhko-88888-детали-e-производитель=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "производитель" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.страна
      class=(if (v-get validationModel "страна" "isInvalid") "error")
      label=(t "forms.i-i-s-snezhko-88888-детали-e.страна-caption")
      data-test-i-i-s-snezhko-88888-детали-e-страна=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "страна" "message")}}
  </div>
</form>
